<Window x:Class="SIMS_Projekat.UI.Dialogs.View.IdMedicineView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SIMS_Projekat.UI.Dialogs.View"
        mc:Ignorable="d"
        Title="IdMedicineView" Height="200" Width="200">
    <Grid>
        <Label Content="Write the id of medicne:"></Label>
        <TextBox Text="{Binding IDMedicine}" Margin="28,48,0,76.4" HorizontalAlignment="Left" Width="123"></TextBox>
        <Button Content="Accept" Command="{Binding AcceptCommand}" Height="43" Margin="28,0,42.6,0.4" VerticalAlignment="Bottom"></Button>
    </Grid>
</Window>
