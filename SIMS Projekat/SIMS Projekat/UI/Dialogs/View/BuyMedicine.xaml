<Window x:Class="SIMS_Projekat.UI.Dialogs.View.BuyMedicine"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SIMS_Projekat.UI.Dialogs.View"
        mc:Ignorable="d"
        Title="BuyMedicine" Height="300" Width="300">
    <Grid>
    <Label Content="Quantity:"/>
        <TextBox Text="{Binding Quantity}" Margin="27,31,52.6,166.4"/>
        <Button Command="{Binding AcceptCommand}" Content="Accept" Margin="27,174,52.6,30.4" />
    </Grid>
</Window>
